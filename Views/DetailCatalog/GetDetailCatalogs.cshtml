@model IEnumerable<CourseWorkFactoryAutomatization.Models.DetailCatalog>

@{
    ViewData["Title"] = "GetDetailCatalogs";
    Layout = "_Layout_Accountant";
}

<h1>Общий список каталогов</h1>

<p>
    <a asp-action="CreateDetailCatalog">Создать новый каталог деталей</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateCreate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateCreate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreate)
            </td>
            <td>
                <a asp-action="EditDetailCatalog" asp-route-id="@item.Id">Редактировать</a> |
                    <a asp-action="InfoDetailCatalog" asp-route-id="@item.Id">Инфо</a> |
                <a asp-action="DeleteDetailCatalog" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>




