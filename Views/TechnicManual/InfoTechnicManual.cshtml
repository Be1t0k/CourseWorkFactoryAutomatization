@model CourseWorkFactoryAutomatization.Models.TechnicManual

@{
    ViewData["Title"] = "InfoTechnicManual";
    Layout = "_Layout_SuperVisor";
}

<h1>InfoTechnicManual</h1>

<div>
    <h4>TechnicManual</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateCreate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateCreate)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                TechnicManualId
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Technics)
        {
            <tr>
                <td>
                    @item.Title
                </td>
                <td>
                    @item.TechnicManualId
                </td>
            </tr>
        }
    </tbody>
</table>
