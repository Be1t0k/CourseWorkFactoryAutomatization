@model CourseWorkFactoryAutomatization.Models.DetailCatalog

@{
    ViewData["Title"] = "Create";
    Layout = "_Layout_Accountant";
}

<h1>Создать каталога деталей</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateDetailCatalog">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DateCreate" class="control-label"></label>
                <input asp-for="DateCreate" class="form-control" />
                <span asp-validation-for="DateCreate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="CreateDetailCatalog" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="GetDetailCatalogs">Назад к общему списку</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
